{
	"$schema": "https://json.schemastore.org/tsconfig",
	"compilerOptions": {
		"downlevelIteration": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"lib": [
			"es2023",
			"dom"
		],
		"module": "Node16",
		"moduleResolution": "Node16",
		"outDir": "./dist",
		"skipLibCheck": true,
		"strict": true,
		"target": "ES2020"
	},
	"exclude": [
		"node_modules/**/*"
	],
	"include": [
		"./**/*.ts"
	]
}